bin_dir "../bin"
compile "clang -Wall -Wextra -Werror -Wno-unused -Wno-multichar -std=gnu11 -O3 -IInc -g -funsigned-char -fsanitize=address,undefined -c %src_file% -o %obj_file%"
link "clang -g -fsanitize=address,undefined %obj_files% -o kemu"

targets
[
	<Main.c> [ <Inc/Common.h>  ]
	<Util.c> [ <Inc/Common.h>  ]
]

dependencies
[
	<Inc/Common.h>
]
